<% include ../partials/upper.ejs %>

<div class="admin-page">
<div class="admin-button">
<button id="startVoting" type="button" class="btn admin-button-1 btn-lg">Start Voting</button>
<button id="endVoting" type="button" class="btn admin-button-2 btn-lg">End Voting</button>
</div>

<div class="form-register" style="margin-top: 50px">
<form class="form-horizontal admin-form" onsubmit="formData(event)">
  <div class="form-group" style="padding-bottom: 25px;">
    <label for="inputEmail1" class="col-sm-2 control-label" style="color: #fff">Party</label>
    <div class="col-sm-10">
      <input type="text" name="partyName" class="form-control" id="inputEmail1" placeholder="Enter Party to register">
    </div>
  </div>
  <div class="form-group" style="padding-bottom: 25px;">
    <label for="inputEmail2" class="col-sm-2 control-label" style="color: #fff">Constituency</label>
    <div class="col-sm-10">
      <input type="text" name="partyConstituency" class="form-control" id="inputEmail2" placeholder="Enter Constituency">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default btn-block btn-primary">Submit</button>
    </div>
  </div>
</form>
</div>
</div>

<% include ../partials/lower.ejs %>
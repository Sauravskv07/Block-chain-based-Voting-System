<% include /partials/upper.ejs %>

<div class="form-register" style="margin-top: 15em">
<form class="form-horizontal admin-form" onsubmit="partyData(event)">
  <div class="form-group" style="padding-bottom: 25px;">
    <label for="inputEmail1" class="col-sm-2 control-label" style="color: #fff">Party</label>
    <div class="col-sm-10">
      <input type="text" name="partytoDisplay" class="form-control" id="party" placeholder="Enter Party to Display">
    </div>
  </div>
  <div class="form-group" style="padding-bottom: 25px;">
    <label for="inputEmail2" class="col-sm-2 control-label" style="color: #fff">Constituency</label>
    <div class="col-sm-10">
      <input type="text" name="constituencytoDisplay" class="form-control" id="const" placeholder="Enter Constituency">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default btn-block btn-primary">Submit</button>
    </div>
  </div>
</form>
<div id="totalVote">
  
</div>
</div>
